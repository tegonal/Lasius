{
  "name": "lasius-frontend",
  "private": true,
  "version": "2.0.0",
  "description": "Lasius NextJS Frontend Application Server",
  "author": "tegonal.com <info@tegonal.com>",
  "license": "AGPL 3.0",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start-local": "next start -p 3001",
    "dev": "next dev -p 3001",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier --check --write 'src/**/*.{ts,tsx,js,d.ts}'",
    "check": "yarn run lint-fix && yarn run prettier && yarn run typecheck",
    "clean": "yarn cache clean --all && rm -rf ./node_modules & rm -rf ./.next & rm -rf ./.swc",
    "cleaner": "yarn run clean && rm yarn.lock && yarn install && yarn rebuild",
    "clean-prod": "yarn workspaces focus --production && yarn cache clean --all",
    "up": "yarn upgrade-interactive",
    "i18n-extract": "i18next",
    "services:start": "docker compose -f docker-compose.dev.yml up -d",
    "services:stop": "docker compose -f docker-compose.dev.yml down",
    "services:clean": "docker compose -f docker-compose.dev.yml down -v --remove-orphans",
    "services:logs": "docker compose -f docker-compose.dev.yml logs -f",
    "_dev-backend-proxy": "docker run --add-host=host.docker.internal:host-gateway -p 3000:3000 -v $(pwd)/nginx-dev-proxy.conf:/etc/nginx/nginx.conf:ro --rm nginx:latest",
    "_dev-backend-db": "docker run -p 27017:27017 --rm mongo:8.0.9",
    "_dev-backend-sbt": "cd ../backend && dotenv -e ../frontend/.env.local -- sbt run -Dconfig.resource=dev.conf",
    "_dev-backend-keycloak": "docker run --add-host=host.docker.internal:host-gateway --rm -p 8080:8080 -v $(pwd)/keycloak-local-realm.json:/opt/keycloak/data/import/lasius-local-realm.json -e KC_BOOTSTRAP_ADMIN_USERNAME=admin -e KC_BOOTSTRAP_ADMIN_PASSWORD=admin quay.io/keycloak/keycloak:26.1.4 start-dev --import-realm",
    "_dev-backend-mailhog": " docker run --rm -p 8025:8025 -p 1025:1025 mailhog/mailhog",
    "backend": "cd ../backend && dotenv -e ../frontend/.env.local -- sbt run -Dconfig.resource=dev.conf",
    "backend-minimal": "cd ../backend && dotenv -e ../frontend/.env.local -- sbt run -Dconfig.resource=dev-minimal.conf",
    "backend-large": "cd ../backend && dotenv -e ../frontend/.env.local -- sbt run -Dconfig.resource=demo-large.conf",
    "orval": "rm -rf ./src/lib/api/lasius && orval --config ./orval.config.js && yarn check",
    "tsc": "tsc --noEmit --incremental --watch",
    "typecheck": "tsc --noEmit",
    "analyze": "ANALYZE=true yarn run build",
    "self-update": "yarn set version latest"
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.2",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@nivo/bar": "^0.99.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/pie": "^0.99.0",
    "@nivo/stream": "^0.99.0",
    "@popperjs/core": "^2.11.8",
    "@vercel/og": "^0.8.5",
    "axios": "^1.13.0",
    "boring-avatars": "^2.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "es-toolkit": "^1.41.0",
    "framer-motion": "^12.23.24",
    "i18next": "^25.6.0",
    "immer": "^10.2.0",
    "js-cookie": "^3.0.5",
    "json-stable-stringify": "^1.3.0",
    "lucide-react": "^0.548.0",
    "negotiator": "^1.0.0",
    "next": "^15.5.6",
    "next-auth": "4.24.11",
    "next-i18next": "^15.4.2",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.8.0",
    "parse-json": "^8.3.0",
    "react": "19.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "19.2.0",
    "react-hook-form": "7.65.0",
    "react-i18next": "^16.2.1",
    "react-popper": "^2.3.0",
    "react-use-websocket": "^4.13.0",
    "satori": "^0.18.3",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.3.1",
    "tslog": "^4.10.2",
    "usehooks-ts": "^3.1.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz",
    "zod": "^4.1.12",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@next/bundle-analyzer": "^15.5.6",
    "@openapitools/openapi-generator-cli": "^2.25.0",
    "@tailwindcss/postcss": "4.1.16",
    "@types/d3-shape": "^3.1.7",
    "@types/js-cookie": "^3.0.6",
    "@types/negotiator": "^0.6.4",
    "@types/node": "24.9.1",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "concurrently": "^9.2.1",
    "daisyui": "^5.3.10",
    "dotenv-cli": "^10.0.0",
    "eslint": "9.38.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-license-header": "^0.8.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "i18next-parser": "^9.3.0",
    "jsonc-eslint-parser": "^2.4.1",
    "orval": "7.15.0",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "svgo": "^4.0.0",
    "tailwindcss": "4.1.16",
    "typescript": "5.9.3"
  },
  "packageManager": "yarn@4.10.3"
}
