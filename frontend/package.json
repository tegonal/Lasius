{
  "name": "lasius-frontend",
  "private": true,
  "version": "1.0.0",
  "description": "Lasius NextJS Frontend Application Server",
  "author": "tegonal.com <info@tegonal.com>",
  "license": "AGPL 3.0",
  "engines": {
    "node": ">=18.7.0"
  },
  "scripts": {
    "build": "yarn run svgstore && next build",
    "start": "next start",
    "dev": "yarn run svgstore &&  next dev -p 3001",
    "svgstore": "node ./scripts/iconNames.js && prettier --check --write './src/types/iconNames.d.ts' && svgstore -o ./public/symbols.svg ./public/icons/**/*.svg",
    "lint": "next lint",
    "lint-fix": "next lint --fix",
    "prettier": "prettier --check --write 'src/**/*.{ts,tsx,js,d.ts}'",
    "clean": "yarn cache clean --all && rm -rf ./node_modules & rm -rf ./.next & rm -rf ./.swc",
    "cleaner": "yarn run clean && rm yarn.lock && yarn install && yarn rebuild",
    "clean-prod": "yarn workspaces focus --production && yarn cache clean --all",
    "up": "yarn upgrade-interactive",
    "i18n-extract": "i18next",
    "_dev-backend-proxy": "docker run --add-host=host.docker.internal:host-gateway -p 3000:3000 -v $(pwd)/nginx-dev-proxy.conf:/etc/nginx/nginx.conf:ro --rm nginx:latest",
    "_dev-backend-db": "docker run -p 27017:27017 --rm mongo:5.0.9",
    "_dev-backend-sbt": "cd ../backend && sbt run -Dconfig.resource=demo.conf",
    "backend": "concurrently --kill-others-on-fail --names \"BACKEND,PROXY,DB\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"sleep 3 && yarn run _dev-backend-sbt\" \"yarn run _dev-backend-proxy\" \"yarn run _dev-backend-db\"",
    "orval": "rm -rf ./src/lib/api/lasius && orval --config ./orval.config.js && yarn run lint-fix",
    "tsc": "tsc --noEmit --incremental --watch",
    "analyze": "ANALYZE=true yarn run build"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@headlessui/react": "^1.7.15",
    "@hookform/error-message": "^2.0.1",
    "@nivo/bar": "^0.83.0",
    "@nivo/core": "^0.83.0",
    "@nivo/pie": "^0.83.0",
    "@popperjs/core": "^2.11.8",
    "@theme-ui/color": "^0.15.7",
    "@theme-ui/match-media": "^0.15.7",
    "axios": "^1.4.0",
    "boring-avatars": "^1.10.1",
    "cookie": "^0.5.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "framer-motion": "^10.12.16",
    "i18next": "^22.5.1",
    "js-cookie": "^3.0.5",
    "json-stable-stringify": "^1.0.2",
    "lodash": "^4.17.21",
    "next": "^13.4.5",
    "next-auth": "4.22.1",
    "next-i18next": "^13.3.0",
    "next-plausible": "^3.7.2",
    "next-pwa": "^5.6.0",
    "next-seo": "^6.0.0",
    "react": "^18.2.0",
    "react-async-hook": "^4.0.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "7.44.3",
    "react-i18next": "^12.3.1",
    "react-popper": "^2.3.0",
    "react-use-websocket": "^4.3.1",
    "swr": "^2.1.5",
    "theme-ui": "^0.15.7",
    "ts-export-to-csv": "^0.3.3",
    "usehooks-ts": "^2.9.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@next/bundle-analyzer": "^13.4.5",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "@types/cookie": "^0.5.1",
    "@types/d3-shape": "^3.1.1",
    "@types/js-cookie": "^3.0.3",
    "@types/json-stable-stringify": "^1.0.34",
    "@types/lodash": "^4.14.195",
    "@types/node": "18.16.18",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "concurrently": "^8.2.0",
    "eslint": "8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-next": "^13.4.5",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-license-header": "^0.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "i18next-parser": "^8.0.0",
    "orval": "^6.16.0",
    "prettier": "^2.8.8",
    "svgo": "^3.0.2",
    "svgstore-cli": "^2.0.1",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.6.0"
}
